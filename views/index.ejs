<html>

<head>
  <meta charset='utf-8'>
  <meta name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no'>
  <link href='https://fonts.googleapis.com/css?family=Roboto+Mono' rel='stylesheet'>
  <link rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css' integrity='sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4'
    crossorigin='anonymous'>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css'>
  <link rel='stylesheet' href='style.css' />

  <title> Shane's Portfolio </title>
</head>

<body>
 <!-- Modal -->
  <% include partials/modal %>
<!-- Navbar -->
  <% include partials/navBar %>
<!-- Body -->
  <img class='landing-img' src='images/index-img.png' alt='Broken Arrow' height=100% width=100% />
  <div class='top-centered title' id='landing-title'>Shane McNair</div>
  <div class='top-subTitle' id='subTitle'></div>
<!-- Footer -->
  <% include partials/footer%>

  <script src='https://code.jquery.com/jquery-3.3.1.slim.min.js' integrity='sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo'
    crossorigin='anonymous'></script>
  <script src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js' integrity='sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49'
    crossorigin='anonymous'></script>
  <script src='https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js' integrity='sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T'
    crossorigin='anonymous'></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/TypewriterJS/1.0.0/typewriter.min.js"></script>

  <script>
    const subTitle = document.getElementById('subTitle');
    var typewriter = new Typewriter(subTitle, {
      loop: true
    });
    const pause = 2;

    typewriter.typeString('Dynamic Problem Solver')
      .pauseFor(pause)
      .deleteAll()
      .typeString('Experienced Trouble Shooter')
      .pauseFor(pause)
      .deleteAll()
      .typeString('JavaScript, SQL, XML/XSLT')
      .pauseFor(pause)
      .deleteAll()
      .typeString('HTML, CSS')
      .pauseFor(pause)
      .deleteAll()
      .typeString('NodeJS, ExpressJS')
      .pauseFor(pause)
      .deleteAll()
      .typeString('React, Redux')
      .pauseFor(pause)
      .deleteAll()
      .start();
  </script>
</body>
</html>